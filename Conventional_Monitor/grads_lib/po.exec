set vpage 0.0 11.0 0.0 8.5
set grads off
set gxout value 
enable print fig.meta
open satwind.ctl

*set lon  180 360 
*set lat   -60 60 

set lon 280 300 
set lat  20 30 

set mproj off

set ccolor 1
d maskout(maskout(qi,799-po),po-750)

set ccolor 2
d maskout(maskout(maskout(qi,suf),799-po),po-750)

print




