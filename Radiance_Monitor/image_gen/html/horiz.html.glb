<SCRIPT LANGUAGE="JavaScript">

function plotMap() {
var ssat_index = document.datacover.sat.selectedIndex;
var ssat = document.datacover.sat[ssat_index].value;
var type_index = document.datacover.field.selectedIndex;
var type = document.datacover.field[type_index].value;
var chan_index = document.datacover.channel.selectedIndex;
var chan = document.datacover.channel[chan_index].value;
var parentnew='pngs/horiz/'+ ssat + '.' + type + '_' + chan + '.png'

parent.fmain.document.location.href=parentnew

}



</SCRIPT></HEAD>



